{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "error": "#F7768E",
    "foreground": "#A9B1D6",
    "green": "#9ece6a",
    "muted_text": "#565F89",
    "primary": "#bb9af7",
    "warning": "#FF9E64",
    "yellow": "#E0D667",
    "blue":"#2ac3de",
    "rust":"#D16A3E"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "{{ .Path }}",
          "foreground": "p:foreground",
          "type": "path",
          "style": "plain",
          "options": {
            "style": "full",
            "folder_separator_icon": "  ",
            "home_icon": " <p:primary>󱂵 </>"
          }
        },
        {
          "template": " 󰭕 {{ .User.Email }} ",
          "foreground": "p:muted_text",
          "type": "git",
          "style": "plain",
          "options": {
            "fetch_user": true
          }
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "overflow": "hide",
      "segments": [
        {
          "type": "go",
          "style": "plain",
          "foreground": "p:blue",
          "template": "  {{ .Full }} "
        },
        {
          "type": "rust",
          "style": "plain",
          "foreground": "p:rust",
          "template": "  {{ .Full }} "
        },
        {
          "template": "\ued0d  {{ .Full }} {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{.PackageManagerName}}{{ end }}",
          "foreground": "p:green",
          "type": "node",
          "style": "plain",
          "options": {
            "fetch_package_manager": true
          }
        },

        {
          "template": " x{{ reason .Code }}",
          "foreground": "p:error",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}p:error{{ end }}"
          ]
        },
        {
          "template": " {{ .FormattedMs }} ",
          "foreground": "p:muted_text",
          "type": "executiontime",
          "style": "diamond",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#E06C75{{ end }}"
          ]
        }

      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "!",
          "foreground": "#E06C75",
          "type": "root",
          "style": "plain"
        },
        {
          "template": " ❯ ",
          "foreground": "p:blue",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 4
}